console.log("Reverse the array using recursion method");
let arr = [10,20];
let n= arr.length;
let i,start,end,temp;
start=0;
end =n-1;


function reverseA(arr,start,end)
{
  if(start>=end)
  return;
  else
  {
    temp =arr[start];
    arr[start] =arr[end];
    arr[end] = temp;
  }
  reverseA(arr,start+1,end-1);
}

reverseA(arr,start,end);
for(i=0;i<n;i++)
console.log(arr[i]);
