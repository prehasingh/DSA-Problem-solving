class Solution 
{
    //Function to find a continuous sub-array which adds up to a given number.
    subarraySum(arr, n, s)
    {
        //your code here
        let i=0,start=0,sum=0;
        for(i=0;i<n;i++)
        {
            sum=sum+arr[i];
            while(sum>s)
            {
                sum =sum -arr[start++]
            }
            if(sum==s)
            return [start+1, i+1];
        }
        return [-1];
        }
        
    }
