{
    //Function to find the maximum money the thief can get.
    findMaxSum(arr, n)
    { 
        // code here
        let i;
        let dp =[];
        dp[0]=arr[0];
        
        if(n==1)
        {
        return dp[0]
        }
        dp[1]= Math.max(arr[0],arr[1]);
        for(i=2;i<n;i++)
        {
            dp[i]= Math.max(arr[i]+dp[i-2],dp[i-1])
        }
        return dp[n-1]
        }
        }
