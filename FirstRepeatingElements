lass Solution {
    // Function to return the position of the first repeating element.
    firstRepeated(arr, n) {
        // your code here
        let map = new Map();
        let i;
        for(i=0;i<n;i++)
        map[arr[i]]=0;
        
       for(i=0;i<n;i++)
        {
            map[arr[i]]++;
        }
         for(i=0;i<n;i++)
        {
            if(map[arr[i]]>1)
            return(i+1);
        }
        return-1;
    }
