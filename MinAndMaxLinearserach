console.log("Linear search approch");
  let arr = [20,10,30,40,50];
  let max=arr[0],min= arr[0];
  function printMinAndMaxArray( arr)
  {
      let i;
      for(i=1;i<arr.length;i++)
      {
          if(max<arr[i])
          max=arr[i];
          
      }
      for(i=1;i<arr.length;i++)
      {
          if(min>arr[i])
          min =arr[i];
      }
      console.log(max);
      console.log(min);
  }
 printMinAndMaxArray(arr);
