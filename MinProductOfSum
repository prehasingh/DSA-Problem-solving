class Solution{
    minValue(arr,brr,n){
         arr.sort(function(a, b){return b - a});
          brr.sort(function(a, b){return a - b});
          let i,sum=0;
          for(i=0;i<n;i++)
          {
              sum =sum+(arr[i]*brr[i]);
          }
        //code here
        return sum;
    }
}
